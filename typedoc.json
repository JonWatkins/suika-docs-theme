{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.tsx"],
  "out": "docs",
  "exclude": ["**/node_modules/**"],
  "githubPages": true,
  "plugin": ["./dist/index.js"],
  "theme": "suika-theme",
  "includeVersion": true,
  "toolbarLinks": [
    {
      "label": "GitHub",
      "url": "https://github.com/JonWatkins/suika-docs-theme",
      "target": "_blank"
    }
  ],
  "keywords": ["suika", "typedoc", "typedoc-theme"]
}
